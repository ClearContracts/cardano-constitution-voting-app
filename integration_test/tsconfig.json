{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "baseUrl": "./", // This must be specified if "paths" is.
    "paths": {
      "@mock/*": ["lib/_mock/*"],
      "@fixtures/*": ["lib/fixtures/*"],
      "@helpers/*": ["lib/helpers/*"],
      "@pages/*": ["lib/pages/*"],
      "@constants/*": ["lib/constants/*"],
      "@datafactory/*": ["lib/datafactory/*"],
      "@types": ["lib/types.ts"],
    }
  }
}
