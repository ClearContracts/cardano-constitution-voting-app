{
  "extends": ["next/core-web-vitals", "next/typescript", "prettier"],
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single", { "avoidEscape": true }],
    "semi": ["error", "always"],
    "@typescript-eslint/explicit-function-return-type": "warn"
  }
}
